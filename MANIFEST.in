# Manifest template

include MANIFEST.in
include README.md
# add CMakeLists.txt
global-include CMakeLists.txt
# add 3rdparty
graft 3rdparty/glog
graft 3rdparty/gflags
graft 3rdparty/gtest
graft 3rdparty/protobuf
exclude 3rdparty/protobuf/src/protoc*
prune 3rdparty/protobuf/python/build
prune 3rdparty/protobuf/python/dist
prune 3rdparty/protobuf/python/*.egg-info
graft 3rdparty/protobuf-cmake
# add sources
recursive-include src *.cc *.h *.in *.proto

global-exclude *.pb.cc *.pb.h *_pb2.py
global-exclude *.swp
global-exclude *.pyc *.pyo *.pyd

